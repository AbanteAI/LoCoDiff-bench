+                            std::mem::size_of::<PointOffsetType>(),
+                            |i| i.cpu_counter(),
+                        ))
+                    })
+                })
+                .filter(move |&id| !visited_list.check_and_update_visited(id))
+                .filter(move |&i| struct_filtered_context.check(i));
+
+            Either::Right(iter)
